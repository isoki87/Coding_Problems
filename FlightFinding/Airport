package com.leo.flightfinder;

import java.util.HashMap;
import java.util.List;
import java.util.Map;

public class Airport {
	private String name;
	private Map<String, DestinationInfo> destinations;
	
	Airport(String s){
		this.name = s.toLowerCase();
		this.destinations = new HashMap<String, DestinationInfo>();
	}
	
}
